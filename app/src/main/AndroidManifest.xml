<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.murad.restaurantapps">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Add_Item_Activity" />
        <activity android:name=".Activity.Service_Activity" />
        <activity android:name=".Activity.Parcel_Activity" />
        <activity android:name=".Activity.Homedelivery_Activity" />
        <activity android:name=".Activity.Search_Activity"></activity>
    </application>

</manifest>